 <div class="card">
   <div class="card-body">
     <div class="d-flex justify-content-start align-items-start">
       <label class="me-3"> <%= t :name %></label>
       <p><%= user.name %></p>
     </div>
     <div class="d-flex justify-content-start align-items-start">
       <label class="me-3"><%= t :email %></label>
       <p><%= user.email %></p>
     </div>
     <div class="d-flex justify-content-start align-items-start">
       <label class="me-3"><%= t :phone %></label>
       <p><%= user.phone %></p>
     </div>
   </div>
 </div>
